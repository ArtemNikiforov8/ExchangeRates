@using ExchangeRates.Models
@model Currency


<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
<h1>@Model.Name</h1>

<table class="table">
    <tr>
        <th>
            <a asp-action="Index">ID</a>
        </th>
        <th>
            <a asp-action="Index">NumCode</a>
        </th>
        <th>
            <a asp-action="Index">CharCode</a>
        </th>
        <th>
            <a asp-action="Index">Nominal</a>
        </th>
        <th>
            <a asp-action="Index">Value</a>
        </th>
        <th>
            <a asp-action="Index">Previous</a>
        </th>
    </tr>
   
    <tr>
        <td>@Model.ID</td>
        <td>@Model.NumCode</td>
        <td>@Model.CharCode</td>
        <td>@Model.Nominal</td>
        <td>@Model.Value</td>
        <td>@Model.Previous</td>
    </tr>
    
</table>
